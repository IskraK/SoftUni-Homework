@model int

@{
    ViewData["Title"] = "Number Generator";
}

<h1>@ViewData["Title"]</h1>

<form asp-action="NumGenerator" method="post">
    <div class="row">
        <div class="col-lg-3">
            <input class="form-control" name="num" id="num" type="number" />
        </div>
        <button class="btn btn-success" type="submit">Submit</button>
    </div>
</form>

<ul>
    @for (int i = 1; i <= Model; i++)
    {
        <li>@i</li>
    }
</ul>
